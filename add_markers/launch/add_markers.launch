<!--
  Add markers to the map.
 -->
<launch>
  <node name="add_markers" pkg="add_markers" type="add_markers" output="screen">
    <rosparam>
      pickup_point_x: -1.0
      pickup_point_y: -1.0
      dropoff_point_x: 2.0
      dropoff_point_y: 3.0
      real_pickup_point_x: -2.0
      real_pickup_point_y:  2.0
      real_dropoff_point_x: 2.5
      real_dropoff_point_y: 3.0
    </rosparam>
  </node>
</launch>
